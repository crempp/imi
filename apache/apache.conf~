# Listen 80
# NameVirtualHost test.chadrempp.com:80

<VirtualHost *:80>
	 ServerName test.chadrempp.com
	 #ServerName localhost
	 # ServerAlias chadrempp.com www.chadrempp.com
	 ServerAdmin crempp@gmail.com
	      
	 #DocumentRoot "/web/test.chadrempp.com/"

     LogFormat "%h %l %u %t \"%r\" %>s %b \"%{Referer}i\" \"%{User-Agent}i\"" combined
     CustomLog /var/log/apache2/test.chadrempp.com.log combined
     LogLevel info
     ErrorLog /var/log/apache2/test.chadrempp.com.err

	 #WSGIDaemonProcess test.chadrempp.com user=www-data group=www-data processes=5 maximum-requests=500 threads=1
     WSGIDaemonProcess test.chadrempp.com user=www-data group=www-data processes=5 maximum-requests=500 threads=1 python-path=/web/test.chadrempp.com/:/web/_pythonenvironments/django-default/lib/python2.6
	 WSGIProcessGroup test.chadrempp.com
	 WSGIScriptAlias / /web/test.chadrempp.com/apache/django.wsgi
	 Alias /media /web/test.chadrempp.com/media/

	 <Directory /web/test.chadrempp.com/media>
              Options FollowSymLinks
              AllowOverride None
         </Directory>

	 <Directory /web/test.chadrempp.com/media>
              Options FollowSymLinks
              AllowOverride None
         </Directory>

	 Alias /admin/media /var/lib/python-support/python2.6/django/contrib/admin/media/
	 <Directory /var/lib/python-support/python2.6/django/contrib/admin/media>
              Options FollowSymLinks
              AllowOverride None
         </Directory>

</VirtualHost>